 class Solution{
    int LastIndex(String s, char p){
        // code here
        int index=-2;
        for(int i=0;i<s.length();i++)
        {
            if(s.charAt(i)==p)
            {
                index=i;
            }
        }
        if(index==-2)
        {
            return -1;
        }
        else{
            return index;
        }
        
    }
}
